{
  "name": "rubber-ducky-live",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--dns-result-order=ipv4first' next dev",
    "build": "NEXT_CACHE_INVALIDATION_TIMESTAMP=$(date +%s) next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean:dev": "rm -rf .next && npm run dev",
    "clean:cache": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "image:info": "node mini-features/image-reader/read-image-metadata.js",
    "image:scan": "node mini-features/image-reader/read-image-metadata.js public/",
    "image:export": "node mini-features/image-reader/read-image-metadata.js public/ --json public-images-metadata.json",
    "image:quick": "./mini-features/image-reader/quick-image-info.sh",
    "version:patch": "node scripts/release.js patch",
    "version:minor": "node scripts/release.js minor",
    "version:major": "node scripts/release.js major",
    "release:dry-run": "node scripts/release.js patch --dry-run",
    "preversion": "npm run lint && npm run build",
    "postversion": "echo \"Version updated to $(node -p 'require(\"./package.json\").version')\"",
    "deploy:test": "./scripts/deploy.sh test",
    "deploy:staging": "./scripts/deploy.sh staging", 
    "deploy:production": "./scripts/deploy.sh production",
    "deploy:test:dry": "./scripts/deploy.sh test --dry-run",
    "deploy:staging:dry": "./scripts/deploy.sh staging --dry-run",
    "deploy:production:dry": "./scripts/deploy.sh production --dry-run"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@auth/mongodb-adapter": "^3.10.0",
    "@eslint/eslintrc": "^3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react-dom": "^19.1.7",
    "assemblyai": "^4.15.0",
    "autoprefixer": "^10.4.21",
    "ccusage": "^16.1.1",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "googleapis": "^156.0.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.539.0",
    "mongoose": "^8.17.1",
    "next": "15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "replicate": "^1.0.1",
    "schema-dts": "^1.1.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "mongodb-memory-server": "^10.2.0",
    "msw": "^2.10.5",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1"
  }
}
